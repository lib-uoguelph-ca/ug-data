<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsd="http://www.w3.org/2000/10/XMLSchema#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://www.example.org/ontology#"
  	xml:base="http://www.example.org/ontology">
  	
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Planet">
    <rdfs:label xml:lang="pl">Planeta</rdfs:label>
    <rdfs:label xml:lang="en">Planet</rdfs:label>
	<rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  
  <owl:Class rdf:ID="Astronaut">
    <rdfs:label xml:lang="pl">Kosmonauta</rdfs:label>
    <rdfs:label xml:lang="en">Astronaut</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Person">
	    <rdfs:label xml:lang="pl">Osoba</rdfs:label>
	    <rdfs:label xml:lang="en">Person</rdfs:label>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SolarSystem">
    <rdfs:label xml:lang="pl">Układ planetarny</rdfs:label>
    <rdfs:label xml:lang="en">Planetary System</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="born_on">
    <rdfs:label xml:lang="en">born on</rdfs:label>
    <rdfs:label xml:lang="pl">urodzony na</rdfs:label>
    <rdfs:range rdf:resource="#Planet"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:ID="distance">
    <rdfs:label xml:lang="en">distance to sun</rdfs:label>
    <rdfs:label xml:lang="pl">odległość od słońca</rdfs:label>
    <rdfs:domain rdf:resource="#Planet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/10/XMLSchema#positiveInteger"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:ID="is_in">
    <rdfs:label xml:lang="en">is in</rdfs:label>
    <rdfs:label xml:lang="pl">znajduje się w</rdfs:label>
    <rdfs:range rdf:resource="#SolarSystem"/>
    <rdfs:domain rdf:resource="#Planet"/>
  </owl:ObjectProperty>

  <Person rdf:ID="JamesSmith">
    <rdfs:label>James Smith</rdfs:label>
    <born_on>
      <Planet rdf:ID="Mars">
	    <rdfs:label xml:lang="en">Mars</rdfs:label>
	    <rdfs:label xml:lang="pl">Mars</rdfs:label>
        <is_in>
          <SolarSystem rdf:ID="HomeSolarSystem">
		    <rdfs:label xml:lang="en">Solar System</rdfs:label>
		    <rdfs:label xml:lang="pl">Układ Słoneczny</rdfs:label>
          </SolarSystem>
        </is_in>
      </Planet>
    </born_on>
  </Person>
  <Astronaut rdf:ID="AnnLenox">
    <rdfs:label>Ann Lenox</rdfs:label>
    <born_on>
      <Planet rdf:ID="Earth">
	    <rdfs:label xml:lang="en">Earth</rdfs:label>
	    <rdfs:label xml:lang="pl">Ziemia</rdfs:label>
        <is_in rdf:resource="#HomeSolarSystem"/>
      </Planet>
    </born_on>
  </Astronaut>
  <Planet rdf:ID="Venus">
    <rdfs:label xml:lang="en">Venus</rdfs:label>
    <rdfs:label xml:lang="pl">Wenus</rdfs:label>
    <is_in rdf:resource="#HomeSolarSystem"/>
  </Planet>
  <Person rdf:ID="ClarkBrown">
    <rdfs:label>Clark Brown</rdfs:label>
    <born_on rdf:resource="#Earth"/>
  </Person>
  <Person rdf:ID="MaryFenix">
    <rdfs:label>Mary Fenix</rdfs:label>
    <born_on rdf:resource="#Venus"/>
  </Person>
</rdf:RDF>

